<% provide(:title, "Home") %>
	<h1>StaticPages#home</h1>
	<h4>Find me in app/views/static_pages/home.html.erb</h4>
	<% if user_signed_in? %>
		<%= gravatar_for( current_user, 120 ) %>
	<% else %>
		<%= link_to '<button type="button" class="btn btn-primary">Sign Up</button>'.html_safe, sign_up_path %>
	<% end %>
